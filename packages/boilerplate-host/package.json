{
  "name": "@daimler/boiler-host",
  "version": "0.3.2",
  "description": "Mercedes-Benz MO360 Frontend Toolkit | Boilerplate (Source)",
  "license": "MIT",
  "author": "Daimler TSS GmbH",
  "config": {
    "AppIcon": "./src/assets/appIcon/logo.png",
    "title": "WebApp",
    "devServer": {
      "host": "localhost",
      "port": "8080",
      "https": false,
      "publicPath": "/",
      "open": true
    },
    "publicPath": "./"
  },
  "scripts": {
    "start": "build=dev webpack serve",
    "build": "npm run clean && build=prod webpack --mode production",
    "build:ci": "npm run clean && build=prod verbose=true webpack --mode production",
    "lint": "npm run lint:eslint && npm run lint:stylelint && npm run lint:prettier",
    "lint:eslint": "eslint --ext ts,tsx src",
    "lint:prettier": "prettier --check \"./**/*\"",
    "lint:stylelint": "stylelint \"src/**/*.(css|scss)\" --syntax scss --allow-empty-input",
    "lint:fix": "npm run lint:fix:eslint && npm run lint:fix:stylelint && npm run lint:fix:postcss && npm run lint:fix:prettier",
    "lint:fix:prettier": "prettier --write \"./**/*\"",
    "lint:fix:postcss": "postcss --config postcss.config.js --env sort-only --no-map --replace \"src/**/*.(css|scss)\"",
    "lint:fix:stylelint": "stylelint \"src/**/*.(css|scss)\" --syntax scss --fix --allow-empty-input",
    "lint:fix:eslint": "eslint --fix --ext ts,tsx src",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Daimler/mo360-ftk.git"
  },
  "engines": {
    "node": ">=12.13.0",
    "npm": ">=6.12.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "Firefox ESR",
    "ie >= 11"
  ],
  "dependencies": {
    "@daimler/ftk-core": "*",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "regenerator-runtime": "~0.13.9"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "~7.16.11",
    "@babel/runtime": "~7.17.8",
    "@hot-loader/react-dom": "~17.0.2",
    "@types/node": "~12.0.0",
    "@types/prop-types": "~15.7.4",
    "@types/react": "~17.0.43",
    "@types/react-dom": "~17.0.14",
    "@typescript-eslint/eslint-plugin": "~5.17.0",
    "@typescript-eslint/parser": "~5.17.0",
    "babel-loader": "^8.2.4",
    "bundle-loader": "^0.5.6",
    "camelcase": "^6.3.0",
    "copy-webpack-plugin": "~6.0.3",
    "cross-env": "~7.0.3",
    "css-loader": "6.7.1",
    "css-minimizer-webpack-plugin": "3.4.1",
    "css-modules-typescript-loader": "~3.0.0",
    "cssjson": "~2.1.3",
    "duplicate-package-checker-webpack-plugin": "~3.0.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "expose-loader": "3.1.0",
    "favicons-webpack-plugin": "5.0.2",
    "file-loader": "~6.2.0",
    "fork-ts-checker-webpack-plugin": "6.2.6",
    "html-webpack-plugin": "5.5.0",
    "mini-css-extract-plugin": "2.6.0",
    "postcss": "~7.0.17",
    "postcss-cli": "^7.1.1",
    "postcss-extend": "~1.0.5",
    "postcss-import": "~12.0.1",
    "postcss-import-sync": "~7.1.4",
    "postcss-loader": "~3.0.0",
    "postcss-nested": "~4.1.2",
    "postcss-preset-env": "~6.7.0",
    "postcss-remove-prefixes": "~1.2.0",
    "postcss-sorting": "~5.0.1",
    "postcss-unprefix": "~2.1.4",
    "prettier": "^2.6.2",
    "react-hot-loader": "^4.13.0",
    "rimraf": "~3.0.2",
    "simple-progress-webpack-plugin": "~1.1.2",
    "style-loader": "3.3.1",
    "stylelint": "^13.6.1",
    "stylelint-config-css-modules": "2.2.0",
    "stylelint-config-recommended": "5.0.0",
    "ts-loader": "~6.0.4",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3",
    "url-loader": "4.1.1",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "~4.2.2"
  }
}
